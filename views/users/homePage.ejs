                  <div class="product__price flex">
                    <span class="new__price">&#8377; <%= refurbishedLaptops[i].combos[0].salePrice %> </span>
                    <span class="old__price">&#8377; <%= refurbishedLaptops[i].combos[0].regularPrice %> </span>
