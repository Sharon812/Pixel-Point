                    <% } else{ %>
                      <div class="product__buttons">
                        <button class="product__btn" disabled>Out of Stock</button>
                      </div>
                    <% } %>
                </div>
              </div>
              <% } %>
            </div>
            <!--product page end-->
          </div>
        </div>

